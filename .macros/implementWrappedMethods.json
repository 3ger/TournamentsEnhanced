[{
  "command": "ssmacro.replace",
  "args": {
      "info": "Replace Not-Implemented Properties Having Both Getters and Setters",
      "find": "(\\w*?)\\s(\\w*?)\\s*\\{\\sget => throw new NotImplementedException\\(\\);\\sset => throw new NotImplementedException\\(\\);\\s\\}",
      "replace": "$1 $2 { get => UnwrappedObject.$2; set => UnwrappedObject.$2 = value; }",
      "all": true,
      "reg": true,
      "flag": "gm"
  }
},
{
  "command": "ssmacro.replace",
  "args": {
      "info": "Replace Not-Implemented Properties Having Only Getter",
      "find": "(\\w*?)\\s(\\w*?)\\s*=> throw new NotImplementedException\\(\\);",
      "replace": "$1 $2 => UnwrappedObject.$2;",
      "all": true,
      "reg": true,
      "flag": "gm"
  }
},
{
  "command": "ssmacro.replace",
  "args": {
      "info": "Replace Nonary Not-Implemented Methods",
      "find": "(\\w*?)\\s(\\w*?)\\(\\)\\s*\\{\\s*throw new NotImplementedException\\(\\);\\s*\\}",
      "replace": "$1 $2() => UnwrappedObject.$2();",
      "all": true,
      "reg": true,
      "flag": "gm"
  }
},
{
  "command": "ssmacro.replace",
  "args": {
      "info": "Replace Unary Not-Implemented Methods",
      "find": "(\\w*?)\\s(\\w*?)\\((\\w*?)\\s(\\w*?)\\)\\s*\\{\\s*throw new NotImplementedException\\(\\);\\s*\\}",
      "replace": "$1 $2($3 $4) => UnwrappedObject.$2($4);",
      "all": true,
      "reg": true,
      "flag": "gm"
  }
},
{
  "command": "ssmacro.replace",
  "args": {
      "info": "Replace Binary Not-Implemented Methods",
      "find": "(\\w*?)\\s(\\w*?)\\((\\w*?)\\s(\\w*?),\\s(\\w*?)\\s(\\w*?)\\)\\s*\\{\\s*throw new NotImplementedException\\(\\);\\s*\\}",
      "replace": "$1 $2($3 $4, $5 $6) => UnwrappedObject.$2($4, $6);",
      "all": true,
      "reg": true,
      "flag": "gm"
  }
}
]