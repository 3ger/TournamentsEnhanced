using TournamentsEnhanced.Models;
using TournamentsEnhanced.Wrappers;

namespace TournamentsEnhanced.Comparer
{
  public class Town[AttributeToCompare]Comparer : TownComparer
  {
    public override int Compare(MBTown x, MBTown y)
    {
      var xRecord = ModState.TournamentRecords[x];
      var yRecord = ModState.TournamentRecords[y];
      var x[AttributeToCompare] = x.Get[AttributeToCompare]Level();
      var y[AttributeToCompare] = y.Get[AttributeToCompare]Level();

      var result = x[AttributeToCompare].CompareTo(y[AttributeToCompare]);

      return x[AttributeToCompare].CompareTo(y[AttributeToCompare]);
    }
  }
}